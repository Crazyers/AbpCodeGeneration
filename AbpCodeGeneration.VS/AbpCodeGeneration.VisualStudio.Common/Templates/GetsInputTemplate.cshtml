@using AbpCodeGeneration.VisualStudio.Common.Model
@inherits RazorEngine.Templating.TemplateBase<CreateFileInput>
using Abp.Extensions;
using Abp.Runtime.Validation;
using @(Model.Namespace).Dto;

namespace @(Model.Namespace).@(Model.DirectoryName).Dto
{
    public class Get@(Model.ClassName)sInput: PagedAndSortedInputDto, IShouldNormalize
    {
        public void Normalize()
        {
            if (Sorting.IsNullOrWhiteSpace())
            {
                Sorting = "Id DESC";
            }
        }
    }
}